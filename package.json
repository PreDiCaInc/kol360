{
  "name": "kol360",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "e2e:local": "pnpm --filter @kol360/e2e test:all:local",
    "e2e:aws": "pnpm --filter @kol360/e2e test:all:aws",
    "e2e:api:local": "pnpm --filter @kol360/e2e test:api:local",
    "e2e:api:aws": "pnpm --filter @kol360/e2e test:api:aws",
    "e2e:web:local": "pnpm --filter @kol360/e2e test:web:local",
    "e2e:web:aws": "pnpm --filter @kol360/e2e test:web:aws",
    "e2e:seed": "pnpm --filter @kol360/e2e seed",
    "e2e:cleanup": "pnpm --filter @kol360/e2e cleanup",
    "e2e:cleanup:all": "pnpm --filter @kol360/e2e cleanup:all",
    "db:migrate": "pnpm --filter @kol360/api exec prisma migrate dev",
    "db:generate": "pnpm --filter @kol360/api exec prisma generate",
    "db:seed": "pnpm --filter @kol360/api exec prisma db seed",
    "db:studio": "pnpm --filter @kol360/api exec prisma studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0"
}
