{
  "name": "kol360",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "db:migrate": "pnpm --filter @kol360/api exec prisma migrate dev",
    "db:generate": "pnpm --filter @kol360/api exec prisma generate",
    "db:seed": "pnpm --filter @kol360/api exec prisma db seed",
    "db:studio": "pnpm --filter @kol360/api exec prisma studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0"
}
